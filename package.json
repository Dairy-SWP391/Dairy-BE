{
  "name": "dairy-be",
  "version": "1.0.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npx nodemon",
    "build": "rimraf ./dist && tsc && tsc-alias",
    "lint": "yarn run eslint:check && yarn run prettier:check && yarn run check-types",
    "start:dev": "cross-env NODE_ENV=development node dist/index.js",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "prepare": "husky",
    "prettier:check": "prettier ./src/**/*.{tsx,ts,js,jsx} --check",
    "prettier:fix": "prettier ./src/**/*.{tsx,ts,js,jsx} --write",
    "eslint:check": "eslint ./src --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "eslint:fix": "eslint ./src --ext .ts,.tsx,.js,.jsx --max-warnings 0 --fix",
    "eslint:check-allow-warning": "eslint ./src --ext .ts,.tsx,.js,.jsx",
    "check-conflict-prettier-eslint": "npx eslint-config-prettier src/App.tsx --print-config",
    "check-types": "tsc --noemit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "@types/crypto-js": "^4.2.2",
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "D": "^1.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.0.1",
    "firebase": "^10.12.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@stylistic/eslint-plugin": "^2.1.0",
    "@tsconfig/node16": "^16.1.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.1",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.11",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@typescript-eslint/types": "^7.8.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.14.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
